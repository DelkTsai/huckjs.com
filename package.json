{
  "name": "huckjs.com",
  "version": "0.1.0",
  "description": "JS Kickstarter",
  "main": "server.js",
  "repository": "https://github.com/huckjs/huckjs.com",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c",
    "rollup": "rollup -c --watch",
    "restart": "pm2 restart huck",
    "start": "./node_modules/.bin/forever -m 5 --minUptime 1000 --spinSleepTime 500 server.js",
    "knit": "npm-run-all --parallel rollup watch",
    "postinstall": "rollup -c",
    "stop": "pm2 stop huck",
    "show": "pm2 show huck",
    "watch": "pm2 start server.js --name huck"
  },
  "engines": {
    "node": "7.4.0",
    "npm": "4.0.5"
  },
  "dependencies": {
    "@huck/browser": "^0.1.1",
    "@huck/core": "^0.1.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "body-parser": "^1.16.0",
    "chalk": "^1.1.3",
    "compression": "^1.6.2",
    "eslint": "^3.14.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "express": "^4.14.1",
    "forever": "^0.15.3",
    "ip": "^1.1.4",
    "livereload": "^0.6.0",
    "lodash": "^4.17.4",
    "multer": "^1.3.0",
    "node-sass-middleware": "^0.11.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.1",
    "pm2": "^2.3.0",
    "postcss": "^5.2.11",
    "postcss-cssnext": "^2.9.0",
    "pug": "^2.0.0-beta10",
    "redis": "^2.6.5",
    "redux": "^3.6.0",
    "rollup": "^0.41.4",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-eslint": "^3.0.0",
    "rollup-plugin-json": "^2.1.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-pug": "^0.1.2",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-sass": "^0.4.8",
    "rollup-plugin-stylus-css-modules": "^1.2.3",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-watch": "^3.2.2",
    "router5": "^4.4.1",
    "socket.io": "^1.7.2",
    "socket.io-client": "^1.7.2",
    "winston": "^2.3.1"
  }
}
